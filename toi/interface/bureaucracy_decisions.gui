					spriteType = "GFX_government_bureaucracy_food_bureaucracy"
				}

			effects = {
				government_bureaucracy_central_bureaucracy_button_click = {
					if = {
						limit = { check_variable = { var = generic_bureaucracy_decision_central_bureaucracy_flag value = 1 } }
						set_variable = { var = generic_bureaucracy_decision_central_bureaucracy_flag value = 0 }
					}
					else = {
						set_variable = { var = generic_bureaucracy_decision_central_bureaucracy_flag value = 1 }
					}
				}
				government_bureaucracy_food_bureaucracy_button_click = {
					if = {
						limit = { has_country_flag = generic_bureaucracy_decision_central_bureaucracy_complete_flag }
						if = {
							limit = { check_variable = { var = generic_bureaucracy_decision_central_bureaucracy_flag value = 1 } }
							set_variable = { var = generic_bureaucracy_decision_central_bureaucracy_flag value = 0 }
						}
						else = {
							set_variable = { var = generic_bureaucracy_decision_central_bureaucracy_flag value = 1 }
						}
					}
				}
			}
		}
