		containerWindowType = {
			name = "generic_bureaucracy_decision_category_gui_window_bg"
			position = { x = 0 y = 0 }
			size = { width = 490 height= 375 }
			margin = { top = 0 bottom = 0 left = 0 right = 0 }
			
			background = {
				name = "Bureaucracy Background"
				spriteType = "GFX_government_bureaucracy_window_background"
			}
			properties = {
				government_bureaucracy_central_bureaucracy_button = {
					frame = ROOT.generic_bureaucracy_decision_central_bureaucracy_flag
				}
			}

			# Icons
				iconType = {
					name = "government_bureaucracy_central_bureaucracy_button"
